# Router connection settings
ROUTER_HOST=router.example.com
ROUTER_USERNAME=admin
ROUTER_PASSWORD=your_router_password
ROUTER_ENABLE_PASSWORD=your_enable_password
ROUTER_PROTOCOL=ssh  # Pode ser 'ssh' ou 'telnet'
ROUTER_SSH_PORT=22   # Porta padrão SSH
ROUTER_TELNET_PORT=23  # Porta padrão Telnet

# Web application credentials
WEB_USERNAME=admin
WEB_PASSWORD=your_web_password

# Flask configuration
FLASK_SECRET_KEY=generate_a_secure_random_key
FLASK_ENV=production 